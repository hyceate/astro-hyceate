{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "noImplicitAny": true,
    "types": [
      "svelte",
      "vidstack/svelte",
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/lib/components/*"
      ],
      "@layouts/*": [
        "src/lib/layouts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ]
    }
  },
}